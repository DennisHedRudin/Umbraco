@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockListItem>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
    var content = (ContentModels.PartnerCard)Model.Content;
}

<div>
    @if (content.PartnerLogo != null)
    {
        <img src="@content.PartnerLogo.Url()" alt="@content.PartnerName" class="w-full object-cover" />
    }
   
</div>